{
    "name": "cwsdigital/twill-metadata",
    "description": "Add SEO Metadata to your Twill.io models.",
    "keywords": [
        "twill",
        "cwsdigital",
        "twill-metadata"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Dwight",
            "homepage": "https://cwsdigital.com",
            "email": "paul@cwsdigital.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "CwsDigital\\TwillMetadata\\TwillMetadataServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "CwsDigital\\TwillMetadata\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "area17/twill": "^3.0",
        "artesaos/seotools": "^1.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.4"
    }
}
